package test;

class ThrowTest {
    static int test(int i) {
        ThrowTest.sink(100 / i);
        return ThrowTest.test(i-1);
    }

    static void main(String[] args) {
        ThrowTest.test(10);
    }

    static void sink(int in) {}

    static void tryTest() {
        try {
            test(10);
        } catch (ArithmeticException e) {
            System.print(e.getMessage());
        }
    }
}