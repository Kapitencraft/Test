package test;

class GenericTest {
    static void main(String[] args) {
        System.print(create("Hello!"));
        System.print(test(5));
        System.print(test(19f));
        System.print(test(14.));
        Supplier<int> abcd = new TestA(10);
        GenericTest.test(abcd.get());
    }

    static <T> T create(T in) {
        return in;
    }

    static <T extends num> T test(T in) {
        return in;
    }

    static <T> T build(T in) {
        T test = in;
        return test;
    }

    static void testA(String in) {
        System.print(in);
    }

    static class TestA<T> implements Supplier<T> {
        final T value;

        TestA(T value) {
            this.value = value;
        }

        T get() {
            return value;
        }
    }

    interface Supplier<T> {
        T get();
    }
}