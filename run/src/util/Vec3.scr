package util;

class Vec3 {
    final int x, y, z;

    Vec3(int x, int y, int z) {
        this.x = x;
        this.y = y;
        this.z = z;
    }

    Vec3 add(Vec3 other) {
        return new Vec3(this.x + other.x, this.y + other.y, this.z + other.z);
    }

    static void main(String[] args) {
        Vec3 v = new Vec3(1, 2, 3);
        Vec3 v1 = new Vec3(4, 5, 6);
        System.print(v + v1);
    }
}
