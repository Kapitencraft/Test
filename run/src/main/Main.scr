package main;

class Main {
    int x;

    static void main(String args) {
        Main main = new Main.A();
        main.x = 10;
        if (main instanceof A a) {
            a.test2();
        }

        print(main.x);
    }

    void test() {
        this.x++;
    }

    class A extends Main {

        void test2() {
            this.x = 0;
        }
    }
}