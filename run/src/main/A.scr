package main;

abstract class A {
    abstract int test(int in);

    class B extends A {
        int test(int in) {
            return in ** 2;
        }
    }

    class C extends A {
        int test(int in) {
            return in + 8;
        }
    }

    static void main(String args) {
        A a = new B();
        System.print(a.test(5));
        a = new C();
        System.print(a.test(5));
    }
}